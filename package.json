{
  "homepage": "http://Valerchixx.github.io/web-react",
  "name": "my-test-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "history": "^5.3.0",
    "lodash.debounce": "^4.0.8",
